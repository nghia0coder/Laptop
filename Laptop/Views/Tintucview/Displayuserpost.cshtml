                                                                                                <div class="box-info__avatar"><img
                                                                                                        src="https://cdn.sforum.vn/sforum/wp-content/plugins/dtv-comments-system/images/shipper-icon.png">
                                                                                                </div>
                                                                                                <p class="box-info__name">
                                                                                                    ${value.customer_name}</p>
                                                                                                <!----></div>
                                                                                            <div class="box-info-separate"></div>
                                                                                            <p class="box-time-cmt">Ngày ${time.day}
                                                                                                lúc ${time.time}</p>
                                                                                        </div>
