            if (count.data == undefined || count.data < 5) {
                document.getElementById("cmt_loadmore").style.display = 'none';
            }
            setCountComment(count)
            const data = await getComment();
            showComment(data)
        });

        function setCountComment(count) {
            let html = "0 Hỏi đáp"
            if (count.data !== undefined) {
                html = count.data + " " + "Hỏi Đáp"
            }

            jQuery("#total_comment").html(html);
        }

        async function loadMore() {
            page = page + 1
            const data = await getComment(page);
            showComment(data)
        }

        function getComment(page) {
            return $.ajax({
                url: `module/comment?page=${page}`,
                type: "GET",
                dataType: 'json',
                data: {
                    page_url: document.URL,
                }
            });
        }

        function handleTime(time) {
            d = new Date(time);

            return {
                "day": d.toLocaleDateString(),
                "time": d.toLocaleTimeString()
            }
